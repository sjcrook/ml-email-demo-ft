{
    "database-name": "%%mlAppName%%-content",
    "schema-database": "%%mlAppName%%-schemas",
    "triggers-database": "%%mlAppName%%-triggers",
    "enabled": true,
    "data-encryption": "off",
    "encryption-key-id": "",
    "retired-forest-count": 0,
    "language": "en",
    "stemmed-searches": "basic",
    "word-searches": true,
    "word-positions": false,
    "fast-phrase-searches": true,
    "fast-reverse-searches": false,
    "triple-index": true,
    "triple-positions": false,
    "fast-case-sensitive-searches": true,
    "fast-diacritic-sensitive-searches": true,
    "fast-element-word-searches": true,
    "element-word-positions": false,
    "fast-element-phrase-searches": true,
    "element-value-positions": false,
    "attribute-value-positions": false,
    "field-value-searches": false,
    "field-value-positions": false,
    "three-character-searches": false,
    "three-character-word-positions": false,
    "fast-element-character-searches": false,
    "trailing-wildcard-searches": true,
    "trailing-wildcard-word-positions": false,
    "fast-element-trailing-wildcard-searches": false,
    "word-lexicons": null,
    "two-character-searches": false,
    "one-character-searches": false,
    "uri-lexicon": true,
    "collection-lexicon": true,
    "reindexer-enable": true,
    "reindexer-throttle": 5,
    "reindexer-timestamp": 0,
    "directory-creation": "manual",
    "maintain-last-modified": false,
    "maintain-directory-last-modified": false,
    "inherit-permissions": false,
    "inherit-collections": false,
    "inherit-quality": false,
    "in-memory-limit": 131072,
    "in-memory-list-size": 256,
    "in-memory-tree-size": 64,
    "in-memory-range-index-size": 8,
    "in-memory-reverse-index-size": 8,
    "in-memory-triple-index-size": 21,
    "in-memory-geospatial-region-index-size": 32,
    "large-size-threshold": 1024,
    "locking": "fast",
    "journaling": "fast",
    "journal-size": 1365,
    "journal-count": 2,
    "preallocate-journals": false,
    "preload-mapped-data": false,
    "preload-replica-mapped-data": false,
    "range-index-optimize": "facet-time",
    "positions-list-max-size": 256,
    "format-compatibility": "automatic",
    "index-detection": "automatic",
    "expunge-locks": "none",
    "tf-normalization": "scaled-log",
    "merge-priority": "lower",
    "merge-max-size": 49152,
    "merge-min-size": 1024,
    "merge-min-ratio": 3,
    "merge-timestamp": 0,
    "retain-until-backup": false,
    "merge-blackouts": null,
    "database-backups": null,
    "fragment-roots": null,
    "fragment-parents": null,
    "element-word-query-throughs": null,
    "phrase-throughs": null,
    "phrase-arounds": null,
    "range-element-indexes": null,
    "range-element-attribute-indexes": null,
    "element-word-lexicons": null,
    "element-attribute-word-lexicons": null,
    "path-namespaces": null,
    "range-path-index": [
        {
            "scalar-type": "string",
            "path-expression": "/email/headers/tofrom-metas/email-from/email",
            "collation": "http://marklogic.com/collation/codepoint",
            "range-value-positions": false,
            "invalid-values": "reject"
        },
        {
            "scalar-type": "string",
            "path-expression": "/email/headers/tofrom-metas/firstname-from/firstname",
            "collation": "http://marklogic.com/collation/codepoint",
            "range-value-positions": false,
            "invalid-values": "reject"
        },
        {
            "scalar-type": "string",
            "path-expression": "/email/headers/tofrom-metas/email-to/email",
            "collation": "http://marklogic.com/collation/codepoint",
            "range-value-positions": false,
            "invalid-values": "reject"
        },
        {
            "scalar-type": "string",
            "path-expression": "/email/headers/tofrom-metas/firstname-to/firstname",
            "collation": "http://marklogic.com/collation/codepoint",
            "range-value-positions": false,
            "invalid-values": "reject"
        },
        {
            "scalar-type": "int",
            "path-expression": "/email/headers/date-metas/year",
            "collation": "",
            "range-value-positions": false,
            "invalid-values": "reject"
        },
        {
            "scalar-type": "dateTime",
            "path-expression": "/(email/headers/date-metas/dateTime | trade/TradeDateTime | transcript/metadata/dateTime)",
            "collation": "",
            "range-value-positions": false,
            "invalid-values": "reject"
        },
        {
            "scalar-type": "date",
